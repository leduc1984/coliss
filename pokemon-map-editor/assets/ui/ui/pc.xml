<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<!-- PC Storage GUI's -->
	<theme name="pc-frame" ref="resizableframe">
		<param name="background"><image>pc-frame</image></param>
		<param name="titleAreaTop"><int>2</int></param>
		<param name="titleAreaLeft"><int>11</int></param>
		<param name="titleAreaRight"><int>-1</int></param>
		<param name="titleAreaBottom"><int>44</int></param>
		
		<param name="closeButtonX"><int>-24</int></param>
		<param name="closeButtonY"><int>17</int></param>
		
		<theme name="title" ref="-defaults">
			<param name="textAlignment"><enum type="alignment">left</enum></param>
			<param name="font"><font>title-font-black</font></param>
			<param name="border"><border>0,0,-90,0</border></param>
		</theme>
		<theme name="dialoglayout" ref="-defaults">
			<param name="border"><border>0</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
					<theme name="editfield" ref="editfield">
						<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
					</theme>

			<param name="namedGaps"><map/></param>
			<theme name="container" ref="-defaults">
				<param name="background"><image>none</image></param>
				<param name="border"><border>0,2,0,0</border></param>
				<theme name="search-dialog" ref="dialoglayout">
					<param name="border"><border>11,0,0,0</border></param>
					<!-- Value -->
					<theme name="label-value-medium" ref="label">
						<param name="background"><image>ui-inputbox.background</image></param>
						<param name="textAlignment"><enum type="alignment">left</enum></param>
						<param name="border"><border>6</border></param>
						<param name="minWidth"><int>160</int></param>
					</theme>
					<theme name="label-value-medium2" ref="label">
						<param name="background"><image>ui-inputbox.background</image></param>
						<param name="textAlignment"><enum type="alignment">left</enum></param>
						<param name="minWidth"><int>174</int></param>
					</theme>
					<theme name="label-value-medium3" ref="label">
						<param name="background"><image>ui-inputbox.background</image></param>
						<param name="textAlignment"><enum type="alignment">left</enum></param>
						<param name="border"><border>6</border></param>
						<param name="minWidth"><int>212</int></param>
					</theme>
					<theme name="label-value-small" ref="label">
						<param name="background"><image>ui-inputbox.background</image></param>
						<param name="textAlignment"><enum type="alignment">left</enum></param>
						<param name="border"><border>6</border></param>
						<param name="minWidth"><int>120</int></param>
					</theme>
					<theme name="label-value-small2" ref="label">
						<param name="background"><image>ui-inputbox.background</image></param>
						<param name="textAlignment"><enum type="alignment">left</enum></param>
						<param name="border"><border>6</border></param>
						<param name="minWidth"><int>96</int></param>
					</theme>
					<theme name="label-value-smallest" ref="label">
						<param name="background"><image>ui-inputbox.background</image></param>
						<param name="textAlignment"><enum type="alignment">center</enum></param>
						<param name="border"><border>1</border></param>
						<param name="minWidth"><int>120</int></param>
					</theme>
					<theme name="label-value-icon" ref="label">
						<param name="background"><image>none</image></param>
						<param name="textAlignment"><enum type="alignment">center</enum></param>
						<param name="border"><border>1</border></param>
						<param name="minWidth"><int>24</int></param>
					</theme>
					<!-- Sorting -->
					<theme name="label-title-normal" ref="label-value-small2">
						<param name="overlay"><image>ui-sort-default.background</image></param>
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<theme name="label-title-desc" ref="label-value-small2">
						<param name="overlay"><image>ui-sort-desc.background</image></param>
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<theme name="label-title-asc" ref="label-value-small2">
						<param name="overlay"><image>ui-sort-asc.background</image></param>
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<!-- Titles -->
					<theme name="label-title-medium" ref="label-value-medium">
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<theme name="label-title-medium2" ref="label-value-medium2">
						<param name="background"><image>ui-header.background</image></param>
						<param name="textAlignment"><enum type="alignment">center</enum></param>

					</theme>
					<theme name="label-title-medium3" ref="label-value-medium3">
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<theme name="label-title-small" ref="label-value-small">
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<theme name="label-title-small2" ref="label-value-small2">
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<theme name="label-title-small2-centered" ref="label-title-small2">
						<param name="textAlignment"><enum type="alignment">center</enum></param>
					</theme>
					<theme name="label-title-smallest" ref="label-value-smallest">
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<theme name="label-title-icon" ref="label-value-icon">
						<param name="background"><image>ui-header.background</image></param>
					</theme>
					<!-- Buttons -->
					<theme name="label-button-medium" ref="label-value-medium">
						<param name="background"><image>ui-button.background</image></param>
					</theme>
					<theme name="label-button-medium2" ref="label-value-medium2">
						<param name="background"><image>ui-button.background</image></param>
					</theme>
					<theme name="label-button-small" ref="label-value-small">
						<param name="background"><image>ui-button.background</image></param>
					</theme>
					<theme name="label-button-small2" ref="button">
						<param name="background"><image>ui-button.background</image></param>
						<param name="minWidth"><int>96</int></param>
					</theme>
					<theme name="label-button-small2-centered" ref="label-button-small2">
						<param name="textAlignment"><enum type="alignment">center</enum></param>
					</theme>
					<theme name="label-button-smallest" ref="label-value-smallest">
						<param name="background"><image>ui-button.background</image></param>
					</theme>
					<theme name="label-button-icon" ref="label-value-icon">
						<param name="background"><image>ui-button.background</image></param>
					</theme>
					<theme name="editfield" ref="editfield">
						<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
					</theme>
					<theme name="valueadjuster" ref="valueadjuster" allowWildcard="true">
						<param name="width"><int>92</int></param>
					</theme>
					
				</theme>
				<theme name="tabbedpane" ref="-defaults">
					<param name="minWidth"><int>230</int></param>
					<param name="background"><image>none</image></param>
					<param name="border"><border>20,0,2,0</border></param>
					<theme name="tabbox" ref="-defaults">
						<theme name="tabbutton" ref="togglebutton">
							<param name="font"><font>alphabeta-border</font></param>
							<param name=""><image>button-small.*</image></param>
						</theme>
						<param name="border"><border>0,0,0,0</border></param>
						<param name="spacing"><int>-20</int></param>
						<param name="alignment"><enum type="alignment">FILL</enum></param>
					</theme>
					 <theme name="scrollControls" ref="-defaults">
						<param name="border"><border>2,5,-98,0</border></param>
						<param name="defaultGap"><dimension>5,5</dimension></param>
						<param name="smallGap"><dimension>8,8</dimension></param>
						<param name="mediumGap"><dimension>10,10</dimension></param>
						<param name="largeGap"><dimension>0,0</dimension></param>
						<param name="namedGaps"><map>
							<param name="scrollButtons"><gap>2</gap></param>
						</map></param>
						<theme name="scrollLeft" ref="button">
							<param name="text"><string>&lt;</string></param>
						</theme>
						<theme name="scrollRight" ref="button">
							<param name="text"><string>&gt;</string></param>
						</theme>
					</theme>
					<theme name="container" ref="-defaults">
						<param name="border"><border>13,98,0,0</border></param>
						 <theme name="dialoglayout" ref="-defaults">
							<param name="defaultGap"><dimension>-3,-3</dimension></param>
							<param name="smallGap"><dimension>8,8</dimension></param>
							<param name="mediumGap"><dimension>10,10</dimension></param>
							<param name="largeGap"><dimension>15,15</dimension></param>
							<param name="namedGaps"><map/></param>
							<theme name="pc-slot" ref="-defaults">
								<param name="background"><image>pc-slot.background</image></param>
								<param name="minWidth"><int>32</int></param>
								<param name="minHeight"><int>32</int></param>
								<theme name="label-dark" ref="label" allowWildcard="true">
									<param name="font"><font>alphabeta</font></param>
									<param name="border"><border>1,-400,0,0</border></param>
									<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
								</theme>
							</theme>
						</theme>
					</theme>
					<param name="inputMap"><inputMapDef>
						<action name="nextTab">CTRL TAB</action>
						<action name="prevTab">CTRL SHIFT TAB</action>
					</inputMapDef></param>
				</theme>
			</theme>
		</theme>
	</theme>
	
	<theme name="partyslot-shiny" ref="-defaults">
		<param name="background"><image>trade-slot-shiny</image></param>
		<param name="border"><border>8</border></param>
		<param name="defaultGap"><dimension>5,5</dimension></param>
		<param name="smallGap"><dimension>8,8</dimension></param>
		<param name="mediumGap"><dimension>10,10</dimension></param>
		<param name="largeGap"><dimension>15,15</dimension></param>
		<param name="namedGaps"><map/></param>
		
		<theme name="label" ref="label" allowWildcard="true">
			<param name="font"><font>pb-dark</font></param>
		</theme>
	</theme>

	<!-- PC Select All Button -->
	<theme name="selectall-button" ref="-defaults" allowWildcard="true">
		<param name="font"><font>alphabeta</font></param>
		<param name="border"><border>7,5</border></param>
		<param name="background"><image>icon.select-all</image></param>
		<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
	</theme>

	<!-- PC Unselect All Button -->
	<theme name="unselectall-button" ref="-defaults" allowWildcard="true">
		<param name="font"><font>alphabeta</font></param>
		<param name="border"><border>7,5</border></param>
		<param name="background"><image>icon.select-none</image></param>
		<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
	</theme>

	<theme name="edit-button" ref="-defaults" allowWildcard="true">
		<param name="font"><font>alphabeta</font></param>
		<param name="border"><border>7,5</border></param>
		<param name="background"><image>icon.edit</image></param>
		<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
		<param name="minWidth"><int>16</int></param>
		<param name="maxWidth"><int>16</int></param>
		<param name="minHeight"><int>16</int></param>
		<param name="maxHeight"><int>16</int></param>
	</theme>
</themes>
